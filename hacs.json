{
  "name": "Namedays Card",
  "description": "A Home Assistant Lovelace card that shows today's namedays from a JSON file.",
  "render_readme": true,
  "resources": [
    {
      "type": "module",
      "url": "/hacsfiles/namedays-card/namedays-card.js"
    },
    {
      "type": "file",
      "url": "/hacsfiles/namedays-card/namedays.json"
    }
  ],
  "version": "1.0.0",
  "homeassistant": "2025.1.4",
  "domains": ["lovelace"],
  "iot_class": "cloud_push"
}
